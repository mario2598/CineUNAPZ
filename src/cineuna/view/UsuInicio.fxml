<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTabPane?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.TilePane?>

<BorderPane prefHeight="500.0" prefWidth="1000.0" stylesheets="@adminStyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cineuna.controller.UsuInicioController">
   <center>
      <JFXTabPane fx:id="tabPaneMovies" BorderPane.alignment="CENTER">
         <tabs>
            <Tab fx:id="tabCartelera" closable="false" text="Cartelera">
               <content>
                  <ScrollPane fitToHeight="true" fitToWidth="true" focusTraversable="false">
                     <content>
                        <TilePane fx:id="tpCartelera" alignment="TOP_CENTER" hgap="15.0" styleClass="background-pane" vgap="15.0">
                           <padding>
                              <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                           </padding></TilePane>
                     </content>
                  </ScrollPane>
               </content>
            </Tab>
            <Tab fx:id="tabProximamente" closable="false" text="Proximamente">
               <content>
                  <ScrollPane fitToHeight="true" fitToWidth="true" focusTraversable="false">
                     <content>
                        <TilePane fx:id="tpProximamente" alignment="TOP_CENTER" hgap="15.0" styleClass="background-pane" vgap="15.0">
                           <padding>
                              <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                           </padding></TilePane>
                     </content>
                  </ScrollPane>
               </content>
            </Tab>
         </tabs>
      </JFXTabPane>
   </center>
</BorderPane>
