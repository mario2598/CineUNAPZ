<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXListView?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane stylesheets="@adminStyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cineuna.controller.AdminUsuariosController">
   <center>
      <StackPane fx:id="holderPane" BorderPane.alignment="CENTER" />
   </center>
   <left>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <AnchorPane styleClass="details-Pane">
               <children>
                  <JFXButton fx:id="btnRefrescar" contentDisplay="GRAPHIC_ONLY" onAction="#btnRefrescarAction" styleClass="pallet-button" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
                     <graphic>
                        <FontAwesomeIconView boundsType="VISUAL" glyphName="REFRESH" size="20" />
                     </graphic>
                  </JFXButton>
               </children>
            </AnchorPane>
            <JFXListView fx:id="listViewUsers" focusTraversable="false" prefHeight="1080.0" prefWidth="300.0" styleClass="customListView" />
         </children>
      </VBox>
   </left>
</BorderPane>
